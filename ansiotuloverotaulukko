import numpy as np
import matplotlib.pyplot as plt

# Your data
X = np.array([1200, 1280, 1360, 1440, 1520, 1600, 1680, 1760, 1840, 1920, 2000,
              2080, 2160, 2240, 2320, 2400, 2480, 2560, 2640, 2720, 2800,
              2880, 2960, 3040, 3120, 3200, 3280, 3360, 3440, 3520, 3600,
              3680, 3760, 3840, 3920, 4000, 4080, 4160, 4240, 4320, 4400,
              4480, 4560, 4640, 4720, 4800, 4880, 4960, 5040, 5120, 5200,
              5280, 5360, 5440, 5520, 5600, 5680, 5760, 5840, 5920, 6000,
              6080, 6160, 6240, 6320, 6400, 6480, 6560, 6640, 6720, 6800,
              6880, 6960, 7040, 7120, 7200, 7280, 7360, 7440, 7520, 7600,
              7680, 7760, 7840, 7920, 8000, 8080, 8160, 8240, 8320, 8400,
              8480, 8560, 8640, 8720, 8800, 8880, 8960, 9040, 9120, 9200,
              9280, 9360, 9440, 9520, 9600, 9680, 9760, 9840, 9920, 10000])

Y = np.array([0.087, 0.102, 0.103, 0.109, 0.123, 0.135, 0.147, 0.156, 0.164, 0.173,
              0.181, 0.188, 0.194, 0.202, 0.21, 0.218, 0.225, 0.232, 0.237, 0.241,
              0.245, 0.249, 0.254, 0.26, 0.266, 0.272, 0.277, 0.282, 0.287, 0.292,
              0.297, 0.301, 0.305, 0.309, 0.313, 0.316, 0.32, 0.323, 0.326, 0.33,
              0.333, 0.335, 0.338, 0.342, 0.345, 0.348, 0.351, 0.354, 0.356, 0.359,
              0.362, 0.364, 0.367, 0.369, 0.372, 0.374, 0.376, 0.378, 0.38, 0.382,
              0.384, 0.386, 0.387, 0.389, 0.391, 0.392, 0.394, 0.396, 0.397, 0.399,
              0.4, 0.401, 0.403, 0.404, 0.405, 0.407, 0.408, 0.409, 0.41, 0.412,
              0.413, 0.414, 0.415, 0.417, 0.419, 0.421, 0.422, 0.424, 0.426, 0.427,
              0.429, 0.43, 0.432, 0.433, 0.435, 0.436, 0.438, 0.439, 0.44, 0.442,
              0.443, 0.444, 0.446, 0.447, 0.448, 0.449, 0.45, 0.452, 0.453, 0.454,
              0.455])

# Fit a linear model in logarithmic space
log_X = np.log(X)
params = np.polyfit(log_X, Y, deg=1)

# Extract the fitted parameters
a, b = params

# Generate predicted values using the fitted model
Y_fit = a * log_X + b

# Plot the original data and the fitted curve
plt.scatter(X, Y, label='Original Data')
plt.plot(X, Y_fit, label='Exponential Fit', color='red')
plt.xlabel('X')
plt.ylabel('Y')
plt.legend()
plt.show()

# Print the fitted parameters
print(f"Fitted Parameters: a = {a}, b = {b}")
